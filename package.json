{
  "name": "drillmaster",
  "version": "3.3.0",
  "description": "Spanish verb conjugation flashcard generator for Anki",
  "main": "index.js",
  "scripts": {
    "generate:mexico": "node scripts/generate-apkg.js --region mexico",
    "generate:mexico:tier1": "node scripts/generate-apkg.js --region mexico --tier 1",
    "generate:mexico:tier2": "node scripts/generate-apkg.js --region mexico --tier 2",
    "generate:mexico:tier3": "node scripts/generate-apkg.js --region mexico --tier 3",
    "generate:mexico:tier4": "node scripts/generate-apkg.js --region mexico --tier 4",
    "generate:mexico:tier5": "node scripts/generate-apkg.js --region mexico --tier 5",
    "generate:all": "node scripts/generate-apkg.js --tier 1-5,all",
    "dev": "python3 -m http.server 8080"
  },
  "dependencies": {
    "jszip": "^3.10.1",
    "sql.js": "^1.13.0",
    "verbo": "^0.0.9"
  },
  "keywords": [
    "spanish",
    "verbs",
    "conjugation",
    "anki",
    "flashcards"
  ],
  "author": "DrillMaster",
  "license": "MIT"
}
